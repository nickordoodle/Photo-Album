<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.rutgers.scarletmail.kp605.photoalbumandroidapp11" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name=".HomeActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="edu.rutgers.scarletmail.kp605.photoalbumandroidapp11.application.PhotoActivity"
            android:parentActivityName=".HomeActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.rutgers.scarletmail.kp605.photoalbumandroidapp11.HomeActivity" />
        </activity>
        <activity
            android:name="edu.rutgers.scarletmail.kp605.photoalbumandroidapp11.application.AlbumSlideshow"
            android:parentActivityName=".HomeActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="edu.rutgers.scarletmail.kp605.photoalbumandroidapp11.HomeActivity" />
        </activity>
    </application>

</manifest>